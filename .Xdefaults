# angvp .Xdefaults
###################
Xcursor.theme: Jimmac
Xft*dpi:	    96
Xft*antialias:	true
*background:    #121212
*foreground:    #F6F6F6
!URxvt settings
URxvt*termName:     xterm-color
URxvt*font:         xft:Monaco:pixelsize=18:antialias=false
!URxvt*font:         xft:Monaco:pixelsize=13
URxvt.imLocale:     es_AR.UTF-8
URxvt*transparent:  true
URxvt*shading:      10
URxvt*scrollBar:    false
URxvt*geometry:     80x27
URxvt.cursorBlink:           True
URxvt*urlLauncher:           firefox 
URxvt*perl-eXT-common:       selection-autotransform,matcher
URxvt*matcher.button:        2
URxvt.tabbed.secondaryScroll: true
URxvt.tabbed.urlLauncher:     firefox
URxvt.tabbed.matcher.button:  2
# include "/home/angvp/.xcolours/angvp"
